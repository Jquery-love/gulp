	function price_compute(type){
		var totalPrice = parseFloat($("input[name=totalPrice]").val());
		var servicePrice = parseFloat($("input[name=servicePrice]").val());
		var extra = $("input[name=extraValPrice]").val();
		var extraValPrice = 0.00;
		if(extra!=null){
			extraValPrice = parseFloat(extra);
		}
		var num = $("#numberVal").val();
		if(type == 0){
			num ++;
			if(num > auctionNum){
				return;
			}
		}else if(type == 1){
			if(num == 1){
				return;
			}else{
				num --;
			}
		}else{
			if(num==null||num==""||num<1){
				num = 1;
			}else if(num > auctionNum){
				num = auctionNum;
			}
		}
		$("span[id=totalPrice]").html((totalPrice * num).toMoney(2, "", ","));
		$("span[id=servicePrice]").html((servicePrice * num).toMoney(2,"",","));
		$("span[id=extraValPrice]").html((extraValPrice * num).toMoney(2, "", ","));
		$('#numberVal').val(num);
		$("#auctionNum").val(num);
		$("#logisticForm input[name='auctionNum']").val(num);
		countFee();
		if(totalPrice*num > parseFloat($("#userQuota").val())) {
